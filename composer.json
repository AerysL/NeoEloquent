{
    "name": "vinelab/neoeloquent",
    "description": "Laravel wrapper for the Neo4j graph database REST interface",
    "keywords": ["neo4j", "ogm", "graph", "database", "neoeloquent"],
    "license": "MIT",
    "authors": [
        {
            "name": "Abed Halawi",
            "email": "abed.halawi@vinelab.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.1.*",
        "illuminate/pagination": "5.1.*",
        "illuminate/events": "5.1.*",
        "illuminate/contracts": "5.1.*",
        "neoxygen/neoclient": "~2.1",
        "nesbot/carbon": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.*",
        "mockery/mockery": "0.9.*"
    },
    "repositories": [

    ],
    "autoload": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-0": {
            "Vinelab\\NeoEloquent\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
