{
    "name": "vinelab/neoeloquent",
    "description": "Laravel wrapper for the Neo4j graph database REST interface",
    "keywords": ["laravel", "neo4j", "graph", "database", "neoeloquent"],
    "license": "MIT",
    "authors": [
        {
            "name": "Abed Halawi",
            "email": "abed.halawi@vinelab.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*",
        "everyman/neo4jphp": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*",
        "orchestra/testbench": "2.1.*",
        "mockery/mockery": "0.9.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git://github.com/orchestral/phpseclib.git"
        }
    ],
    "autoload": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-0": {
            "Vinelab\\NeoEloquent\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
